{
    "compilerOptions": {
        "baseUrl": "./",
        "target": "ES2023",
        "lib": [
            "ES2023"
        ],
        "noEmit": true,
        "strict": true,
        "rootDir": "./",
        "module": "ESNext",
        "skipLibCheck": true,
        "noUnusedLocals": true,
        "isolatedModules": true,
        "noUnusedParameters": true,
        "moduleDetection": "force",
        "moduleResolution": "bundler",
        "noFallthroughCasesInSwitch": true,
        "allowImportingTsExtensions": true,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "vite.config.ts"
    ]
}