{
    "semi": true,
    "tabWidth": 4,
    "indentSize": 4,
    "useTabs": false,
    "printWidth": 100,
    "singleQuote": false,
    "proseWrap": "always",
    "requireConfig": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "bracketSameLine": false,
    "editor.formatOnSave": true,
    "plugins": ["prettier-plugin-tailwindcss"]
}
